{
	"name": "hypr-contract",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"start": "npm run clean && npm run build && ttab -a Terminal -g npx env-cmd -f .env.local cross-env-shell 'npx hardhat node'",
		"test": "env-cmd -f .env.local cross-env-shell 'hardhat test --deploy-fixture --network localhost'",
		"test:deployments": "env-cmd -f .env.local cross-env-shell 'hardhat test --deploy-fixture --network localhost \"./test/Deployments.spec.ts\"'",
		"test:hypertoken": "env-cmd -f .env.local cross-env-shell 'hardhat test --deploy-fixture --network localhost \"./test/HyperToken.spec.ts\"'",
		"test:spacemaster": "env-cmd -f .env.local cross-env-shell 'hardhat test --deploy-fixture --network localhost \"./test/SpaceMaster.spec.ts\"'",
		"test:strategycaptain": "env-cmd -f .env.local cross-env-shell 'hardhat test --deploy-fixture --network localhost \"./test/StrategyCaptain.spec.ts\"'",
		"test:farmcommander": "env-cmd -f .env.local cross-env-shell 'hardhat test --deploy-fixture --network localhost \"./test/FarmCommander.spec.ts\"'",
		"build": "env-cmd -f .env.local cross-env-shell 'hardhat compile'",
		"clean": "env-cmd -f .env.local cross-env-shell 'hardhat clean && rimraf dist/*.localhost.json' && hardhat run scripts/jsons.ts",
		"deploy": "env-cmd -f .env.local cross-env-shell 'hardhat deploy --network localhost'",
		"deploy:reset": "env-cmd -f .env.local cross-env-shell 'hardhat deploy --reset --network localhost'",
		"deploy:testnet": "env-cmd -f .env.testnet cross-env-shell 'hardhat deploy --reset'"
	},
	"devDependencies": {
		"@babel/core": "^7.13.14",
		"@babel/eslint-parser": "^7.13.14",
		"@babel/preset-env": "^7.13.12",
		"@nomiclabs/hardhat-ethers": "^2.0.2",
		"@nomiclabs/hardhat-etherscan": "^2.1.1",
		"@nomiclabs/hardhat-solhint": "^2.0.0",
		"@nomiclabs/hardhat-waffle": "^2.0.1",
		"@openzeppelin/contracts": "^3.4.0-solc-0.7",
		"@openzeppelin/contracts-upgradeable": "^3.4.0-solc-0.7",
		"@openzeppelin/test-helpers": "^0.5.10",
		"@tenderly/hardhat-tenderly": "^1.0.11",
		"@typechain/ethers-v5": "^6.0.5",
		"@typechain/hardhat": "^1.0.1",
		"@types/chai": "^4.2.16",
		"@types/chai-as-promised": "^7.1.3",
		"@types/jest": "^26.0.23",
		"@types/mocha": "^8.2.2",
		"@types/node-fetch": "^2.5.9",
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"@uniswap/lib": "*",
		"axios": "^0.21.1",
		"chai": "^4.3.4",
		"chai-as-promised": "^7.1.1",
		"cross-env": "^7.0.3",
		"env-cmd": "^10.1.0",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-import-resolver-typescript": "^2.4.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.3.1",
		"ethereum-waffle": "^3.3.0",
		"ethers": "^5.1.0",
		"fp-ts": "^1.19.3",
		"hardhat": "^2.1.2",
		"hardhat-abi-exporter": "^2.2.0",
		"hardhat-deploy": "^0.7.2",
		"hardhat-deploy-ethers": "^0.3.0-beta.7",
		"hardhat-gas-reporter": "^1.0.4",
		"hardhat-jest-plugin": "^0.0.3",
		"hardhat-preprocessor": "^0.1.3",
		"hardhat-spdx-license-identifier": "^2.0.3",
		"hardhat-watcher": "^2.1.1",
		"jest": "^26.6.3",
		"prettier": "^1.19.1",
		"prettier-plugin-solidity": "^1.0.0-beta.7",
		"rimraf": "^3.0.2",
		"solc": "^0.7.6",
		"solhint": "^3.3.4",
		"solhint-plugin-prettier": "^0.0.5",
